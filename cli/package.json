{
  "name": "@somaui/cli",
  "version": "1.0.3",
  "description": "A CLI tool for SomaUI",
  "keywords": [
    "cli",
    "somaui",
    "tool"
  ],
  "homepage": "https://github.com/ikegwuonu/somaui",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikegwuonu/somaui.git"
  },
  "bugs": {
    "url": "https://github.com/ikegwuonu/somaui/issues"
  },
  "bin": {
    "somaui": "dist/bin/somaui.cjs"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "dist/bin/somaui.cjs",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/bin/somaui.ts src/scripts/copy-assets.ts --format cjs --platform node --target node18 --out-dir dist --clean --minify && node dist/scripts/copy-assets.cjs",
    "dev": "tsup src/bin/somaui.ts --format cjs --platform node --target node18 --out-dir dist --clean --watch",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "cpy": "^12.1.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "commander": "^14.0.2"
  }
}
